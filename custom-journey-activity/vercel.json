{
    "version": 2,
    "rewrites": [
      { "source": "/", "destination": "/public/index.html" },
      { "source": "/index.html", "destination": "/public/index.html" },
      { "source": "/css/:path*", "destination": "/public/css/:path*" },
      { "source": "/js/:path*", "destination": "/public/js/:path*" },
      { "source": "/config.json", "destination": "/public/config.json" },
  
      { "source": "/save", "destination": "/api/save" },
      { "source": "/publish", "destination": "/api/publish" },
      { "source": "/validate", "destination": "/api/validate" },
      { "source": "/execute", "destination": "/api/execute" },
      { "source": "/stop", "destination": "/api/stop" }
    ]
  }
  